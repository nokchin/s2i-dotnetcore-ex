@{
  ViewData["Title"] = "Home Page";
  a = 0xfffffffff;
  x = 0xffffffff;   // browser displays value  '4,294,967,295'  for "x" variable here.
  y;
  z;
  y = x + 1;    // browser displays value '0' for "y" variable here, because overflow happens.
  z = x + 2;    // browser displays value '1' for "z" variable here, because overflow happens.
}


a = @a<br>
x = @x<br>
<p>y = @y</p>
<p>z = @z</p>